<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homefrag"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Background"
    tools:context=".fragments.UserProfileFragment">

    <ImageView
        android:id="@+id/foodImgEdit"
        android:layout_width="177dp"
        android:layout_height="168dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/editfood"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/HomeImgEdit"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/HomeImgEdit"
        android:layout_width="177dp"
        android:layout_height="168dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/edithome"
        app:layout_constraintEnd_toStartOf="@+id/foodImgEdit"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/GroceryImgEdit"
        android:layout_width="177dp"
        android:layout_height="168dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/editgros"
        app:layout_constraintEnd_toStartOf="@+id/FurnitureImgEdit"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HomeImgEdit" />

    <ImageView
        android:id="@+id/FurnitureImgEdit"
        android:layout_width="177dp"
        android:layout_height="168dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/editfer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/GroceryImgEdit"
        app:layout_constraintTop_toBottomOf="@+id/foodImgEdit" />

    <TextView
        android:id="@+id/welcomeTxt"
        android:layout_width="141dp"
        android:layout_height="49dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="84dp"
        android:text="@string/list"
        android:textAlignment="textStart"
        android:textColor="#2E2E2E"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/moto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:text="@string/listmoto"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcomeTxt" />

    <ImageView
        android:id="@+id/HomeMainImg"
        android:layout_width="249dp"
        android:layout_height="291dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/housingmainvector"
        app:layout_constraintStart_toEndOf="@+id/welcomeTxt"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>